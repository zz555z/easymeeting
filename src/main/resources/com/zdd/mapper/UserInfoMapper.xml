<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zdd.entry.mapper.UserInfoMapper">

    <resultMap id="BaseResultMap" type="com.zdd.entry.domain.UserInfo">
            <result property="userId" column="user_id" />
            <result property="email" column="email" />
            <result property="nickName" column="nick_name" />
            <result property="sex" column="sex" />
            <result property="password" column="password" />
            <result property="status" column="status" />
            <result property="createTime" column="create_time" />
            <result property="lastLoginTime" column="last_login_time" />
            <result property="lastOffTime" column="last_off_time" />
            <result property="mettingNo" column="metting_no" />
    </resultMap>

    <sql id="Base_Column_List">
        user_id,email,nick_name,sex,password,status,
        create_time,last_login_time,last_off_time,metting_no
    </sql>
</mapper>
