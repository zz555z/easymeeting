<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zdd.entry.mapper.UserContactMapper">

    <resultMap id="BaseResultMap" type="com.zdd.entry.domain.UserContact">
        <id property="userId" column="user_id"/>
        <result property="contactId" column="contact_id"/>
        <result property="status" column="status"/>
        <result property="lastUpdateTime" column="last_update_time"/>
    </resultMap>

    <sql id="Base_Column_List">
        user_id
        ,contact_id,status,last_update_time
    </sql>

    <!-- 批量插入方法 -->
    <insert id="saveBatch">
        INSERT INTO user_contact (user_id, contact_id, status)
        VALUES
        <foreach collection="list" item="item" separator=",">
            (#{item.userId}, #{item.contactId}, #{item.status})
        </foreach>
    </insert>


</mapper>
